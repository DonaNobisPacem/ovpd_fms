<%= simple_form_for(@financial_entry) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :alobs %>
    <%= f.input :description %>
    <%= f.input :party_involved %>
    <%= f.input :transaction_date %>
    <%= f.input :obligation_incurred %>
    <%= f.input :fund_source %>
    <%= f.association :financial_year %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
